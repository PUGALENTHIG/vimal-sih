            @using (Html.BeginForm("Index", "Home", FormMethod.Post, new { id = "frmSearch", @class = "clearfix" }))
            {
                <div class="call-to-action border-5px wow fadeInLeft animation-delay4" style="padding: 16px 16px 0 16px">
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Doctor name</label>
                                @Html.TextBoxFor(s => s.DoctorName, new { placeholder = "Doctor Name", @class = "form-control" })
                            </div>
                        </div>
                        <div class="col-md-3">
                            <label>Select multiple diseases</label>
                            <select name="DiseasesIds" id="DiseasesIds" class="form-control" multiple="multiple">
                                @{
                                    var diseases = CommonFunctions.GetDiseasesDropdownData();

                                    var i = 0;

                                    foreach (var item in diseases)
                                    {
                                        if (i == 0)
                                        {
                                            <option disabled>@item.Text</option>
                                        }
                                        else
                                        {
                                            if (Model.DiseasesIds != null && Model.DiseasesIds.Any() && Model.DiseasesIds.Contains(item.Value))
                                            {
                                                <option value="@item.Value" selected>@item.Text</option>
                                            }
                                            else
                                            {
                                                <option value="@item.Value">@item.Text</option>
                                            }
                                        }


                                        i = i + 1;
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-md-3">
                            <label>Select multiple speaciality</label>
                            <select name="SpeacialityIds" id="SpeacialityIds" class="form-control" multiple="multiple">
                                @{
                                    var speciality = CommonFunctions.GetSpecialityDropdownData();

                                    var k = 0;

                                    foreach (var item in speciality)
                                    {
                                        if (k == 0)
                                        {
                                            <option disabled>@item.Text</option>
                                        }
                                        else
                                        {
                                            if (Model.SpeacialityIds != null && Model.SpeacialityIds.Any() && Model.SpeacialityIds.Contains(item.Value))
                                            {
                                                <option value="@item.Value" selected>@item.Text</option>
                                            }
                                            else
                                            {
                                                <option value="@item.Value">@item.Text</option>
                                            }
                                        }

                                        k = k + 1;
                                    }
                                }
                            </select>
                        </div>
                        <div class="col-md-2">
                            <label>&nbsp;</label>
                            <button type="submit" class="btn btn-info btn-lg btn-block no-border" style="margin-top:3px">Filter data</button>
                        </div>
                    </div>
                </div>
            }

