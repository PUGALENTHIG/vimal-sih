        <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAlDR9mt7M9AIWan51rLETsEsp07wwADDk&libraries=geometry"></script>
        <script type="text/javascript">


            var locations = JSON.parse($("#mapObject").val());

            function initialize() {
                var LatLng = new google.maps.LatLng('22.311595', '73.165528');
                var mapOptions = {
                    center: LatLng,
                    zoom: 13,
                    mapTypeId: google.maps.MapTypeId.ROADMAP
                };

                var map = new google.maps.Map(document.getElementById("dvMap"), mapOptions);
                var marker = new google.maps.Marker({
                    position: LatLng,
                    map: map,
                    icon: '../../Images/mapicon/user.png'
                });

                var infowindow = new google.maps.InfoWindow();
                var i;

                for (i = 0; i < locations.length; i++) {
                    marker = new google.maps.Marker({
                        position: new google.maps.LatLng(locations[i].Lat, locations[i].Long),
                        map: map,
                        icon: '../../Images/mapicon/medic.png'
                    });

                    google.maps.event.addListener(marker, 'click', (function (marker, i) {
                        return function () {
                            infowindow.setContent('<form method="get" action="/Patient/Appointment/' + locations[i].DoctorId + '"> <p style="font-family: \'Open Sans\', sans-serif"><i class="fa fa-user-md text-theme-colored"></i> <b>Name</b>: ' + locations[i].Name + '</p>' +
                                '<p style="font-family: \'Open Sans\', sans-serif"><i class="fa fa-stethoscope text-theme-colored"></i> <b>Speciality</b>: ' + locations[i].Speciality + '</p>' +
                                '</form>');
                            //infowindow.setContent(locations[i].Name);
                            infowindow.open(map, marker);
                        }
                    })(marker, i));

                    google.maps.event.addListener(marker, 'mouseover', (function (marker, i) {
                        return function () {
                            infowindow.setContent('<form method="get" action="/Patient/Appointment/' + locations[i].DoctorId + '"> <p style="font-family: \'Open Sans\', sans-serif"><i class="fa fa-user-md text-theme-colored"></i> <b>Name</b>: ' + locations[i].Name + '</p>' +
                                '<p style="font-family: \'Open Sans\', sans-serif"><i class="fa fa-stethoscope text-theme-colored"></i> <b>Speciality</b>: ' + locations[i].Speciality + '</p>' +
                                '</form>');
                            //infowindow.setContent(locations[i].Name);
                            infowindow.open(map, marker);
                        }
                    })(marker, i));

                    google.maps.event.addListener(marker, 'mouseout', (function (marker, i) {
                        return function () {
                            infowindow.close();
                        }
                    })(marker, i));
                }
            }

            google.maps.event.addDomListener(window, "load", initialize);

            var NearBy = function () {
                if (navigator.geolocation) {
                    navigator.geolocation.getCurrentPosition(function (p) {
                        console.log(p);
                        var LatLng = new google.maps.LatLng(p.coords.latitude, p.coords.longitude);
                        var mapOptions = {
                            center: LatLng,
                            zoom: 13,
                            mapTypeId: google.maps.MapTypeId.ROADMAP
                        };

                        var map = new google.maps.Map(document.getElementById("dvMap"), mapOptions);
                        var marker = new google.maps.Marker({
                            position: LatLng,
                            map: map,
                            icon: '../../Images/mapicon/user.png'
                        });

                        var infowindow = new google.maps.InfoWindow();
                        var i;

                        for (i = 0; i < locations.length; i++) {

                            var distance = google.maps.geometry.spherical.computeDistanceBetween(new google.maps.LatLng(p.coords.latitude, p.coords.longitude), new google.maps.LatLng(locations[i].Lat, locations[i].Long));
                            console.log(distance);
                            if (distance < 5000) {
                                //Distance -- 1000 == 1 KM
                                marker = new google.maps.Marker({
                                    position: new google.maps.LatLng(locations[i].Lat, locations[i].Long),
                                    icon: '../../Images/mapicon/medic.png',
                                    map: map
                                });

                                google.maps.event.addListener(marker, 'click', (function (marker, i) {
                                    return function () {
                                        infowindow.setContent('<form method="get" action="/Patient/Appointment/' + locations[i].DoctorId + '"> <p style="font-family: \'Open Sans\', sans-serif"><i class="fa fa-user-md text-theme-colored"></i> <b>Name</b>: ' + locations[i].Name + '</p>' +
                                            '<p style="font-family: \'Open Sans\', sans-serif"><i class="fa fa-stethoscope text-theme-colored"></i> <b>Speciality</b>: ' + locations[i].Speciality + '</p>' +
                                            '</form>');
                                        //infowindow.setContent(locations[i].Name);
                                        infowindow.open(map, marker);
                                    }
                                })(marker, i));

                                google.maps.event.addListener(marker, 'mouseout', (function (marker, i) {
                                    return function () {
                                        infowindow.close();
                                    }
                                })(marker, i));

                            } else {
                                $("#customDoctor").find("li").each(function (e, element) {
                                    if ($(this).attr("data-docname").toLowerCase() == locations[i].Name.toLowerCase() && $(this).attr("data-profession").toLowerCase() == locations[i].Speciality.toLowerCase()) {
                                        $(this).remove();
                                    }
                                });
                            }
                        }

                        //<button class="btn btn-dark btn-theme-colored mt-15" type="submit">Contact Dr.</button>
                    });
                } else {
                    alert('Geo Location feature is not supported in this browser.');
                }
            };

            var CallDoctor = function (id) {
                $.get("demo_test.asp", function (data, status) {
                    alert("Data: " + data + "\nStatus: " + status);
                });
            };

            var showError = function (error) {
                console.log("ss");
                initialize();
            };
        </script>
